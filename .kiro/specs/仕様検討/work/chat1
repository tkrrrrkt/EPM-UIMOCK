6. ãƒ‡ãƒ¼ã‚¿ãƒ¢ãƒ‡ãƒ«ï¼ˆè¿½åŠ ï¼‰

-- ãƒãƒƒãƒ”ãƒ³ã‚°ãƒ†ãƒ³ãƒ—ãƒ¬ãƒ¼ãƒˆ
CREATE TABLE import_mapping_templates (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  tenant_id UUID NOT NULL REFERENCES tenants(id),
  company_id UUID NOT NULL REFERENCES companies(id),
  name VARCHAR(100) NOT NULL,
  import_category VARCHAR(20) NOT NULL, -- 'AMOUNT' | 'MASTER'
  import_type VARCHAR(20) NOT NULL,     -- 'BUDGET', 'ACCOUNT', etc.
  column_mappings JSONB NOT NULL,
  created_by UUID NOT NULL REFERENCES users(id),
  created_at TIMESTAMPTZ DEFAULT NOW(),
  updated_at TIMESTAMPTZ DEFAULT NOW(),
  UNIQUE(tenant_id, company_id, name)
);

-- å–è¾¼å±¥æ­´ï¼ˆæ‹¡å¼µï¼‰
ALTER TABLE import_batches ADD COLUMN company_id UUID REFERENCES companies(id);
ALTER TABLE import_batches ADD COLUMN import_category VARCHAR(20);
ALTER TABLE import_batches ADD COLUMN original_file_path VARCHAR(500);
ALTER TABLE import_batches ADD COLUMN excluded_rows_count INT DEFAULT 0;
ALTER TABLE import_batches ADD COLUMN mapping_template_id UUID REFERENCES import_mapping_templates(id);
